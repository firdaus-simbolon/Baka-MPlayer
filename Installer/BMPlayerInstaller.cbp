<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="BMPlayerInstaller" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="bin">
				<Option platforms="Windows;" />
				<Option output="bin\BMPlayerInstaller" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-fexpensive-optimizations" />
					<Add option="-Os" />
					<Add option="-O3" />
					<Add option="-O2" />
					<Add option="-O1" />
					<Add option="-O" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Linker>
			<Add library="wsock32" />
		</Linker>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
